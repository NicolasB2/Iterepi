<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iterepi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.login.RegisterMenuActivity"></activity>
        <activity android:name=".view.login.RegisterUserEmailActivity" />
        <activity android:name=".view.SectionStoreUser" />
        <activity android:name=".view.store.StoreHomeActivity" />
        <activity android:name=".view.user.UserFeedActivity" />
        <activity android:name=".view.AddToCartDialog" />
        <activity android:name=".view.SellDetailDialog" />
        <activity android:name=".view.user.UserProfileActivity" />
        <activity android:name=".view.PurchaseDetailDialog" />
        <activity android:name=".view.login.MainActivity" />
        <activity android:name=".view.login.LoginStoreActivity" />
        <activity android:name=".view.login.LoginUserActivity" />
        <activity android:name=".view.login.LoginUserEmailActivity" />
        <activity android:name=".view.login.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>